    "angular-notifier": "^4.1.1",
    "angular-pipes": "^9.0.2",
    "async": "^3.1.0",
    "auto-launch": "^5.0.5",
    "bootstrap": "^4.3.1",
    "bower": "^1.8.8",
    "check-disk-space": "^2.0.0",
    "chokidar": "^3.1.1",
    "core-js": "^2.6.1",

    "electron-browser-storage": "^1.0.6",
